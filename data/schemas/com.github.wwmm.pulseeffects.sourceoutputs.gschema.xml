<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="com.github.wwmm.pulseeffects.sourceoutputs" path="/com/github/wwmm/pulseeffects/sourceoutputs/">
        <key name="plugins" type="as">
          <default>
              ["gate","multiband_gate","webrtc","limiter","compressor",
               "multiband_compressor","filter","equalizer","deesser","reverb",
               "pitch"]
          </default>
        </key>
        <key name="buffer-pulsesrc" type="i">
            <range min="1" max="10000000"/>
            <default>200000</default>
        </key>
        <key name="latency-pulsesrc" type="i">
            <range min="1" max="10000000"/>
            <default>10000</default>
        </key>
        <key name="buffer-pulsesink" type="i">
            <range min="1" max="10000000"/>
            <default>200000</default>
        </key>
        <key name="latency-pulsesink" type="i">
            <range min="1" max="10000000"/>
            <default>10000</default>
        </key>
    </schema>
</schemalist>
